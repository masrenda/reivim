{
  "name": "vue-snippets",
  "displayName": "Vue 2 Snippets",
  "author": "hollowtree",
  "publisher": "hollowtree",
  "icon": "img/logo.png",
  "description": "A Vue.js 2 Extension",
  "license": "MIT",
  "version": "0.1.12",
  "engines": {
    "vscode": "^1.5.0"
  },
  "keywords": [
    "javascript",
    "snippet",
    "vue",
    "vue 2"
  ],
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hollowtree/vscode-vue-snippets"
  },
  "bugs": {
    "url": "https://github.com/hollowtree/vscode-vue-snippets/issues",
    "email": "smalltree@outlook.com"
  },
  "contributes": {
    "languages": [
      {
        "id": "vue",
        "aliases": [
          "Vue",
          "vue"
        ],
        "extensions": [
          ".vue"
        ]
      }
    ],
    "grammars": [
      {
        "language": "blade",
        "scopeName": "text.html.php.blade",
        "path": "./laravel-blade-snippets-vscode/syntaxes/blade.tmLanguage.json",
        "embeddedLanguages": {
          "source.php": "php",
          "source.css": "css",
          "source.js": "javascript"
        }
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./vscode-vue-snippets/snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./vscode-vue-snippets/snippets/javascript.json"
      },
      {
        "language": "html",
        "path": "./vscode-vue-snippets/snippets/html.json"
      },
